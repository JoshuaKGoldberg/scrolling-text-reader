{
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"noEmit": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2022"
	},
	"include": ["src", "vite-env.d.ts"]
}
